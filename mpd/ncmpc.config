scroll-offset = 1
set-xterm-title = yes
xterm-title-format = "[[%artist% - ]%title%]|%file%"

enable-colors = yes
color background = none
color title = none/14
color title-bold = none,bold/14
color line = none/14
color line-flags = magenta
color list = black
color list-bold = blue,bold
color browser-directory = black
color browser-playlist = black
color progressbar = black/14
color progressbar-background = black
color status-song = blue,bold/14
color status-state = none,bold/14
color status-time = black
color alert = red
